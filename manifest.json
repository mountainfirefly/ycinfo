{
  "name": "ycInfo",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "",
  "homepage_url": "http://vaibhavmule.com/ycinfo",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Show info",
    "default_popup": "popup.html"
  },
  "permissions": ["declarativeContent", "activeTab"]
}